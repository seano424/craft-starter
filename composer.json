{
    "require": {
        "craftcms/cms": "4.3.6.1",
        "craftcms/redactor": "3.0.3",
        "nystudio107/craft-retour": "4.1.9",
        "nystudio107/craft-seomatic": "4.0.19",
        "putyourlightson/craft-blitz": "4.2.3",
        "putyourlightson/craft-sprig": "2.5.0",
        "sebastianlenz/linkfield": "2.1.5",
        "spatie/craft-ray": "^2.0",
        "trendyminds/craft-design-tokens": "1.0.3",
        "trendyminds/craft-palette": "3.1.0",
        "verbb/navigation": "2.0.16",
        "verbb/super-table": "3.0.7",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "craftcms/generator": "^1.0",
        "laravel/pint": "^1.2",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\guide\\": "modules/guide/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
